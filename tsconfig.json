{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"],
    "typeRoots": [
      "../node_modules/@types",
      "../@types"
    ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
